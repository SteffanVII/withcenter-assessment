{
  "name": "withcenter-assessment",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build::server": "turbo run build --filter=@repo/blog-types --filter=blog-server",
    "build::webapp": "turbo run build --filter=@repo/blog-types --filter=blog-webapp",
    "dev": "turbo run dev --parallel --force",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.7.4",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=18"
  }
}
